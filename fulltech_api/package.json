{
  "name": "fulltech-api",
  "version": "0.1.1",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "postinstall": "prisma generate",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "sql:migrate": "node -e \"require('./dist/scripts/runSqlMigrations').runSqlMigrations().then(()=>process.exit(0)).catch(e=>{console.error(e);process.exit(1)})\"",
    "migrate:new": "node scripts/create-migration.js",
    "migrate:fix-checksums": "node scripts/fix-migration-checksums.js",
    "create-pos-tables": "tsx scripts/create_pos_module_tables.ts",
    "reset-password": "tsx scripts/reset_user_password.ts",
    "create-crm-tables": "tsx scripts/create_crm_whatsapp_tables.ts",
    "create-ai-tables": "tsx scripts/create_ai_settings_and_suggestions_tables.ts",
    "consolidate-empresa": "tsx scripts/consolidate_to_single_empresa.ts",
    "audit-empresas": "tsx scripts/audit_empresas.ts",
    "delete-other-empresas": "tsx scripts/delete_other_empresas.ts",
    "delete-instance-chats": "tsx scripts/delete_instance_number_chats.ts",
    "clear-crm-chats": "tsx scripts/clear_crm_chats.ts",
    "crm:verify": "tsx scripts/verify_crm_integrity.ts",
    "crm:check-multi-instance": "node scripts/check_and_apply_crm_multi_instance_sql.js",
    "maintenance:verify": "tsx scripts/verify_maintenance_schema.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "ffmpeg-static": "^5.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.13.1",
    "puppeteer": "^23.11.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.10",
    "@types/pdfkit": "^0.17.4",
    "@types/pg": "^8.11.11",
    "prisma": "^5.20.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
